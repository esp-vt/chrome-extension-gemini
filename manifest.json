{
  "manifest_version": 3,
  "name": "Gemini API Chrome Extension",
  "version": "1.0",
  "description": "A Chrome extension to interact with Gemini API",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://api.openweathermap.org/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
